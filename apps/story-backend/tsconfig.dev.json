{
  "extends": "./tsconfig.json",
  "ts-node": {
    "transpileOnly": true,
    "ignore": [],
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "strict": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true
  }
}
